{
  "name": "Static Website",
  "description": "Simple HTML/CSS/JavaScript website for landing pages and prototypes",
  "type": "static-site",
  "language": "javascript",
  "deployment": {
    "targets": ["gitlab-pages"],
    "default": "gitlab-pages"
  },
  "devServer": {
    "port": 8181,
    "command": "npm run dev"
  },
  "build": {
    "required": true,
    "command": "npm run build:sitemap",
    "outputDir": "public"
  },
  "requirements": {
    "node": ">=18.0.0",
    "python": false,
    "environment": []
  },
  "features": [
    "Pure HTML/CSS/JavaScript",
    "No build tooling required",
    "Fast development",
    "Simple deployment to GitLab Pages",
    "Perfect for prototypes and landing pages"
  ]
}
